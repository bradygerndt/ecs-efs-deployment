[
    {
      "name": "factorio",
      "image": "factoriotools/factorio",
      "cpu": 4,
      "memory": 512,
      "essential": true,
      "networkMode": "awsvpc",
      "portMappings": [
        {
          "containerPort": 34197,
          "hostPort": 34197
        }
      ],
      "mountPoints" : [
          {
              "containerPath": "/factorio",
              "sourceVolume": "factorio-storage"
          }
      ]
    }
  ]