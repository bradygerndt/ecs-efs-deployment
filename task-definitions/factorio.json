[
  {
    "name": "factorio",
    "image": "factoriotools/factorio:stable",
    "cpu": 4,
    "memory": 1024,
    "essential": true,
    "networkMode": "awsvpc",
    "portMappings": [
      {
        "containerPort": 34197,
        "hostPort": 34197,
        "Protocol": "udp"
      },
      {
        "containerPort": 27015,
        "hostPort": 27015
      }
    ],
    "mountPoints": [
      {
        "containerPath": "/factorio",
        "sourceVolume": "factorio-storage",
        "ReadOnly": false
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "secretOptions": null,
      "options": {
        "awslogs-group": "/ecs/factorio-ecs-logs",
        "awslogs-region": "us-east-2",
        "awslogs-stream-prefix": "ecs"
      }
    },
    "entryPoint": [
      "bash",
      "-c",
      "echo 'testPassword' > factorio/config/rconpw && /docker-entrypoint.sh"
    ]
  }
]